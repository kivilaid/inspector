  "installCommand": "npm install",
  "framework": null,
  "devCommand": "cd client && npm run dev -- --port $PORT",
  "env": {
    "NODE_VERSION": "22"
  },
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}